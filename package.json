{
    "name": "bot-template",
    "version": "1.1.0",
    "description": "A Discord bot template using TypeScript, Prisma, and Discord.js",
    "scripts": {
        "build": "rimraf dist && tsc",
        "start": "node dist/index.js",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "prettier": "prettier --write **/*.{ts,js,json}",
        "cmd:glob": "node dist/functions/deploy/deployGlobal.js",
        "cmd:dev": "node dist/functions/deploy/deployDev.js"
    },
    "type": "module",
    "main": "dist/index.js",
    "private": true,
    "author": "zueripat <ruedepatrick@gmail.com>",
    "dependencies": {
        "@discordjs/core": "^1.1.1",
        "@prisma/client": "^5.7.1",
        "discord-api-types": "^0.37.67",
        "dotenv": "^16.3.1",
        "pino": "^8.17.2",
        "rimraf": "^5.0.5"
    },
    "devDependencies": {
        "@types/node": "^20.10.6",
        "@typescript-eslint/eslint-plugin": "^6.18.1",
        "@typescript-eslint/parser": "^6.18.1",
        "eslint": "^8.56.0",
        "eslint-config-neon": "^0.1.57",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "^3.1.1",
        "prisma": "^5.7.1",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=17.0.0"
    },
    "packageManager": "yarn@4.0.2"
}
